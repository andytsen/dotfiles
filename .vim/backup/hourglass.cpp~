#include <iostream>
using namespace std;

int findHourGlass(int r, int c, int* foo);

int main() {
    int foo[6][6];

    for(int i = 0; i < 6; i++) {
        for(int j = 0; j < 6; j++) {
            cin >> foo[i][j];
        }  
    }

    int high = 0;
    for(int r = 0; r <= 3; r++) {
        for(int c = 0; c <= 3; c++) {
            int val = findHourGlass(r, c, foo);
        }
    } 



}

int findHourGlass(int r, int c, int** foo) {
    return foo[r][c] + foo[r][c + 1] + foo[r][c + 2] + 
        foo[r + 1][c + 1] + foo[r + 2][c] + foo[r + 2][c + 1] + foo[r + 2][c + 2];
    
}
