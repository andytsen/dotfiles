#include <iostream>
using namespace std;

int main() 
{
    int a1, b1, c1, a2, b2, c2;
    cin >> a1 >> b1 >> c1 >> a2 >> b2 >> c2;

    if(b1 == 0 || b2 == 0)
    {
        if(b1 == 0 && a2 == 0)
        {
            if(a1 == 0 && c1 != 0)
            {

            }
            else if(b2 == 0 && a1 == 0)
            {

            }
        }
    }
    else 
    {
        double slope1 = a1/b1;
        double slope2 = a2/b2;
        // simple case
        if(slope1 == slope2)
        {
            if(c1 == c2)
            {
                cout << -1 << endl;
            }
            else 
            {
                cout << 0 << endl;
            }
        }
        else 
        {
            cout << 1 << endl;
        }
    }
    return 0;


}
