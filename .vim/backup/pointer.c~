#include <stdlib.h>
#include <string.h>
#include<stdio.h>
// screwing around in pointers. 


int main() {
    int *ptr;
    int x = 5;
    *ptr = x;
    printf("this is value of x: %x", x);
    printf("this is value of *ptr: %x", ptr);
    printf("this is value of &x: %x", &x);
    printf("this is value of ptr: %x", ptr);
    return 0;
}
