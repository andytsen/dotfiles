public class coin {

    public static void main(String[] args) {
       p31(); 
    }

    public static void p31() {
        int start = 0;
        int[] amnt = {0};
        recur31(amnt, start);
        System.out.println(amnt[0]);
    }

    public static  void recur31(int[] amnt, int start) {
        if(start == 200) {
             amnt[0]++;
        }
        if(start > 200) {
            return;
        } 

        recur31(amnt, start + 1);
        recur31(amnt, start + 2);
        recur31(amnt, start + 5);
        recur31(amnt, start + 10);
        recur31(amnt, start + 20);
        recur31(amnt, start + 50);
        recur31(amnt, start + 100);
        recur31(amnt, start + 200);
    } 

}
