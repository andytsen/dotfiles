#include "Ship.h"

Ship::Ship() {
    symbol = '#';
    x = 0;
    y = 0;
}

Ship::Ship(int a, int b, char sym) {
    symbol = sym;
    x = a;
    y = b;
} 

void Ship :: timeStep(int ticks) {
    // undefined 
}

void Ship :: setShape(char c) {
    symbol = c;
}

char Ship :: getShape() {
    return symbol;
}
