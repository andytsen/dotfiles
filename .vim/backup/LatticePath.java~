public class LatticePath {
	
	public static void main(String[] args) {
		System.out.println(problem());
	}

	public static int problem() {
		int[][] map = new int[20][20];
		map[19][19] = 1;
		return problemRecur(map, 0, 0);
	}

	public static int problemRecur(int[][] map, int row, int col) {
		
		if (row > 19 || col > 19)
			return 0;
		if (map[row][col] == 1)
			return 1;
		
		return problemRecur(map, row + 1, col) + problemRecur(map, row, col + 1);
	}
}



