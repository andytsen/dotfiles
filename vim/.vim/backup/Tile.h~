#ifndef TILE_H_INCLUDED
#define TILE_H_INCLUDED
#include "GameObject.h"

class Tile : public GameObject {
    public:
        Tile();
        Tile(char s) : symbol(s) {}
        Tile(int, int);
        virtual void setShape(char);
        virtual char getShape();
        virtual void timeStep(int);
    protected:
        char symbol;
};

#endif
