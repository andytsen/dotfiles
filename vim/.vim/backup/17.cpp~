#include <iostream>
#include <string>
using namespace std;

int main() 
{
    unsigned len = 0;
    string teen[19] = {"one", "two", "three", "four", "five", "six", "seven"
    , "eight", "nine", "ten", "eleven", "twelve", "thirteen", "fourteen", 
    "fifteen", "sixteen", "seventeen", "eighteen", "nineteen"};

    for(int i = 0; i < 9; i++)
    {
        len += teen[i].length();
    }
    int single = len; // 36?
    int teens = 0;
    for(int i = 10; i < 19; i++)
    {
        teens += teen[i].length(); 
    }
    // 67?
    cout << "single: " << single << endl;
    cout << "teens: " << teens << endl;



    
}
