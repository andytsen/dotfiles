#include <iostream>
#include <string>
using namespace std;

int main() {
    int t;
    cin >> t;
    int d = 0;
    for(int i = 0; i < t; i++)
    {
        string c;
        int a, b;
        cin >> c >> a >> b;
        if(a >= 2400)
        {
            if((a - b) > 0)
            {
                d = 1;
                break;
            }
                
        }
    }
    if(d)
    {
        cout << "YES" << endl;
    }
    else 
    {
        cout << "NO" << endl;
    }
    return 0;
}
