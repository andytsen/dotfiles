#include "GameObject.h"

bool GameObject :: isCollision(GameObject& other) {
    return ( (other.getY() == y) && (other.getX() == x)); 
}

void GameObject :: timeStep(int tick) {
    // undefined
}

char GameObject :: getShape() {
    // also undefined
    return 'a';
}


void GameObject :: setX(int a) {
    x = a;
}

void GameObject :: setY(int b) {
    y = b;
}

void GameObject :: decY() {
    y--;
}

void GameObject :: decX() {
    x--;
}

void GameObject :: incY() {
    y++;
}


void GameObject :: incX() {
    x++;
}

int GameObject :: getX() const {
    return x;
}

int GameObject :: getY() const {
    return y;
}
