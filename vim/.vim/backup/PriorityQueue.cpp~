#include "PriorityQueue.h" /// include the header file for the priority queue
using namespace std;

// override this method as it is the only different thing. 

PriorityQueue::PriorityQueue() {
    PriorityQueue::LinkedList();
}

PriorityQueue :: ~PriorityQueue() {
    PriorityQueue::~LinkedList();
}

void PriorityQueue :: push(Node* n) {
    Node* curr = head; 
    Node* prev = head; 
    while(1) {
        prev = curr;
        curr = head->next;
        if(curr->value > n->value) {
            prev->next = n;
            n->next = curr; 
            break; 
        }
    }
}




